<header>

  <div class="header__menu">
    {% for link in linklists.main-menu.links %}
      {{ link.title | link_to: link.url }}
    {% endfor %}
  </div>

  <a href="{{ routes.root_url }}" class="logo-link header__title">
    <img class="logo-brand" src="{{ 'logo-pressmeon.svg' | asset_url }}" alt="{{ shop.name }}" width="124" height="24">
  </a>



  <div class="header__icons">
    {% if shop.customer_accounts_enabled %}
      <a href="{{ routes.account_url }}" class="account-link" aria-label="Account">
        {% comment %} {{ 'icon-account.svg' | inline_asset_content }} {% endcomment %}
        Account
      </a>
      
    {% endif %}

    <a href="/cart" class="cart_menus cart-icon">
      Cart (
      {% if cart.item_count > 0 %}
        <sup>{{ cart.item_count }}</sup>
      {% endif %})
